{
  "name": "slacksupport",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "tsx watch server.ts",
    "expose": "ngrok http 5173",
    "start": "react-router-serve ./build/server/index.js",
    "start:custom": "node --import tsx server.ts",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@react-router/node": "7.10.1",
    "@react-router/serve": "7.10.1",
    "@slack/web-api": "^7.13.0",
    "@types/express": "^5.0.6",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.66.1",
    "express": "^5.2.1",
    "ioredis": "^5.8.2",
    "isbot": "^5.1.31",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "7.10.1",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@react-router/dev": "7.10.1",
    "@tailwindcss/vite": "^4.1.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "dotenv": "^17.2.3",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.21.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.16"
  }
}
